{
    "name": "dkplus/csrf-api-unprotection-bundle",
    "description": "Disables the CSRF-token validation for all urls that matches a given expression.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Bley",
            "email": "oskar@programming-php.net"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Dkplus\\CsrfApiUnprotectionBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DkplusTest\\CsrfApiUnprotectionBundle\\": "test"
        }
    },
    "require": {
        "php": "^5.6|^7",
        "symfony/config": "^2.8|^3|^4",
        "symfony/dependency-injection": "^2.8|^3|^4",
        "symfony/form": "^2.8|^3|^4",
        "symfony/http-foundation": "^2.8|^3|^4",
        "symfony/http-kernel": "^2.8|^3|^4",
        "beberlei/assert": "^2||^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "ocramius/finalizer": "^1.0"
    }
}
